{
	"name": "echo",
	"version": "1.0.0",
	"type": "commonjs",
	"main": "main.js",
	"author": "IBM ECHO",
	"description": "IBM ECHO Rest API client",
	"scripts": {
		"electron-start-dev": "cd server/ && yarn run electron-start-dev",
		"electron-build": "cd server/ && yarn make",
		"dev": "vite",
		"build": "vite build",
		"build:prod": "vite build",
		"lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"dev-server-proxy": "export IS_ELECTRON_BUILD=false && npx kill-port 3000 && local-ssl-proxy --source 3000 --target 3001 & env-cmd -f server/.env node --inspect server/index.cjs",
		"dev-server": "export IS_ELECTRON_BUILD=false && cd server && yarn dev-server",
		"prod-server": "export IS_ELECTRON_BUILD=false && env-cmd -f server/.env.prod node server/index.cjs",
		"prettier": "prettier . --write",
		"clean-install": "rm -rf node_modules && yarn install && cd server && rm -rf node_modules && yarn install --save-dev"
	},
	"dependencies": {
		"@carbon/react": "^1.52.0",
		"@monaco-editor/react": "^4.6.0",
		"axios": "^1.7.4",
		"carbon-components-react": "^8.37.0",
		"connect-history-api-fallback": "^2.0.0",
		"cookie": "0.4.0",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"electron-squirrel-startup": "^1.0.0",
		"express": "^4.17.2",
		"express-session": "^1.17.2",
		"jsonpath": "^1.1.1",
		"nanoid": "^4.0.2",
		"react": "^18.2.0",
		"react-autocomplete-input": "^1.0.30",
		"react-dom": "^18.2.0",
		"react-draggable": "^4.4.6",
		"react-resizable-layout": "^0.7.0",
		"react-router": "^6.15.0",
		"react-router-dom": "^6.15.0",
		"react-xarrows": "^2.0.2"
	},
	"devDependencies": {
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@vitejs/plugin-react-swc": "^3.3.2",
		"env-cmd": "^10.1.0",
		"eslint": "^8.45.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"prettier": "3.1.1",
		"sass": "^1.75.0",
		"vite": "^4.4.5"
	},
	"license": "Apache-2.0"
}
